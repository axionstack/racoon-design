    "sass:watch": "sass --load-path=node_modules --style=compressed src/scss/racoon.scss:assets/css/racoon.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axionstack/racoon-design.git"
  },
