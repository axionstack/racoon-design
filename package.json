{
  "name": "racoon-design",
  "version": "0.1.0",
  "description": "A Bootstrap 4.6 Theme",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:clean": "rm -rf dist",
    "build": "npm run build:clean && npm run sass && npm run webpack",
    "webpack": "webpack --mode production",
